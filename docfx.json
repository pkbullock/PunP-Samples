{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md",
          "contributing/*.md",
          "contributing/toc.yml"
        ]
      },
      {
        "files":[
          "toc.yml",
          "**.md",
          "**/**.md"
        ],
        "src": "../scripts",
        "dest": ""
      }
    ],
    "resource": [
      {
        "files": [
          "assets/**"
        ]
      },
      {
        "files":[
          "**/assets/*.png",
          "**/assets/*.jpg",
          "**/assets/*.gif",
          "**/assets/sample.json"
        ],
        "src": "../scripts",
        "dest": ""
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "PnPun Samples",
      "_appFooter": "<span>PnPun Samples<br>Generated by <strong><a href='https://dotnet.github.io/docfx' alt='Doc FX Website'>DocFX</a></strong> with <strong><a href='https://ovasquez.github.io/docfx-material' alt='DocFx Material Theme'>Material UI</a></strong></span>",
      "_appFaviconPath": "https://c.s-microsoft.com/favicon.ico?v2",
      "_appLogoPath": "assets/logo.png",
      "_description":"I tried to Jazz up the Site, but all I got was blues.",
      "_enableSearch": false,
      "_enableNewTab": true,
      "_disableAffix": true,
      "_disableBreadcrumb": true,
      "_disableNavbar": false,
      "_gitContribute": {
      }
    }
  }
}